{
  "name": "vite-reactts-eslint-prettier",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:prettier": "prettier --check ./src",
    "lint:eslint": "eslint ./src --max-warnings=0 --ignore-path ./.gitignore",
    "lint:staged": "lint-staged",
    "commit:check:js": "tsc && pnpm lint:prettier && pnpm lint:eslint",
    "commit:check:css": "stylelint --max-warnings=0",
    "docker:dev": "docker-compose up"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@tanstack/react-query": "^4.24.4",
    "@tanstack/react-query-devtools": "^4.24.4",
    "axios": "^1.3.2",
    "dotenv": "^16.0.3",
    "lint-staged": "^13.1.1",
    "postcss": "^8.4.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.43.1",
    "react-router-dom": "^6.8.1",
    "socket.io": "^4.6.0",
    "zod": "^3.20.6",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-check-file": "^1.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "husky": "^8.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "stylelint": "^15.1.0",
    "stylelint-config-clean-order": "^5.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-css-modules-no-global-scoped-selector": "^1.0.2",
    "stylelint-csstree-validator": "^2.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "ts-css-modules-vite-plugin": "^1.0.20",
    "typescript": "^4.5.5",
    "vite": "^2.9.12"
  },
  "pre-commit": "lint",
  "license": "MIT"
}
